# My aliases

# Git
alias gits='git status'
alias gita='git add .'
alias gitc='git commit'
alias gitl='git log --oneline'
alias gitf='git fetch'
alias gitpo='git push origin'
alias gitpom='git push origin master'
alias g14="g++ -std=c++14 -O2 -Wall"

# Clipboard
alias pbcopy='xsel --clipboard --input'
alias pbpaste='xsel --clipboard --output'
alias copy='pbcopy <'

# For activating virtual environments
alias cvenv='source ~/py-virtual-envs/py3cv4/bin/activate'
alias gisenv='source ~/py-virtual-envs/py3gis/bin/activate'
alias flaskenv='source ~/py-virtual-envs/flaskenv/bin/activate'
alias mlenv='source ~/py-virtual-envs/mlenv/bin/activate'
alias djenv='source ~/py-virtual-envs/djenv/bin/activate'

# Safe delete
alias rm='rm -i'

# Python3
alias py='python3'

